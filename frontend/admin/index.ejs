<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <link rel="stylesheet" href="background.css" type="text/css" />
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
  integrity="sha512-iecdLmaskl7CVkqkXNQ/ZH/XLlvWZOJyj7Yy7tcenmpD1ypASozpmT/E0iPtmFIB46ZmdtAc9eNBvH0H/ZpiBw=="
  crossorigin="anonymous" referrerpolicy="no-referrer" type="text/css" />
  <title>Admin System</title>
  <link rel="icon" href="data:image/jpeg;base64," type="image/icon type">
  <link rel="stylesheet" href="style.css" type="text/css" />
  <link rel="stylesheet" href="mystyle.css" type="text/css">
</head>

<body>
  <section id="main">
    <div class="air air1"></div>
    <div class="air air2"></div>
    <div class="wrapper">
      <div class="onEvent">
        <h1>ADMIN PAGE</h1>
      </div>
      <div class="btn-grid">
        <div class="home-btn">
          <a href="#add">
            <button>ADD EVENT</button>
          </a>
        </div>
        <div class="home-btn">
          <a href="#delete">
            <button>DELETE EVENT</button>
          </a>
        </div>
      </div>
    </div>


    <div class="air air3"></div>
    <div class="air air4"></div>
  </section>


  <section class="add" id="add">
    <div class="air air1"></div>
    <div class="air air2"></div>

    <div class="wrapper">
      <div class="onEvent">
        <h1>Add Upcoming Event</h1>
      </div>
      <form method="post" action="/notify" enctype="multipart/form-data">
        <div class="addInfo">
          <label for="clubs">Choose a Club:</label>
          <div class="select-container">
            <select name="clubName" id="clubName" defaultValue="" class="select-box">
              <option value="none" selected disabled hidden>Select</option>
              <option value="ACES">ACES</option>
              <option value="CSI">CSI</option>
              <option value="DSAII">MESA</option>
              <option value="DevKraft">TESA</option>
              <option value="Rotract">TESA</option>
              <option value="GDSC">TESA</option>
            </select>
          </div>

          <!-- <h3>Event Name</h3> -->
          <div class="field">
            <input name="description" placeholder="Enter Event Name" type="text" required />
          </div>

          <!-- <h3>Date</h3> -->
          <div class="field">
            <input type="date" name="date" placeholder="Enter Date" required />
          </div>

          <!-- <h3>Image</h3> -->
          <div class="field">
            <input type="file" name="image" required />
          </div>

          <div class="field btn">
            <div class="btn-layer"></div>
            <input type="submit" value="Add Event" />
          </div>
        </div>
      </form>
    </div>

    <div class="air air3"></div>
    <div class="air air4"></div>

  </section>

  <section class="delete" id="delete">
    <div class="air air1"></div>
    <div class="air air2"></div>
    <div class="wrapper">
      <div class="onEvent">
        <h1>Delete Event</h1>
      </div>
      <div class="addInfo">
        <form method="post" action="/delete">
          <label for="clubs">Choose a Club:</label>
          <div class="select-container">
            <select name="del" id="clubName" defaultValue="" class="select-box">
              <option value="none" selected disabled hidden>Select</option>
              <option value="ACES">ACES</option>
              <option value="CSI">CSI</option>
              <option value="MESA">MESA</option>
              <option value="TESA">TESA</option>
            </select>
          </div>

          <!-- <h3>Event Name</h3> -->
          <div class="field">
            <input name="delete" placeholder="Enter Event Name" type="text" required />
          </div>

          <div class="field btn">
            <div class="btn-layer"></div>
            <input type="submit" value="Delete Event" />
          </div>
        </form>
      </div>
    </div>



    <div class="air air3"></div>
    <div class="air air4"></div>
  </section>

  <div class="up-btn">
    <a href="#main"><i class="fas fa-chevron-up fa-2x"></i></a>
  </div>
</body>

</html>